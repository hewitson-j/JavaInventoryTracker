<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="450.0" minWidth="600.0" prefHeight="650.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javainventorytracker.EditItemViewController">
   <children>
      <Label fx:id="pageHeaderText" alignment="CENTER" layoutX="232.0" layoutY="32.0" prefHeight="35.0" prefWidth="335.0" text="Edit Item" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="210.0" layoutY="154.0" prefHeight="17.0" prefWidth="178.0" text="Name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="productNameInput" layoutX="210.0" layoutY="174.0" prefHeight="25.0" prefWidth="379.0">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Label layoutX="210.0" layoutY="244.0" prefHeight="17.0" prefWidth="178.0" text="Price">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="productPriceInput" layoutX="210.0" layoutY="264.0" prefHeight="25.0" prefWidth="379.0">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <HBox alignment="CENTER" layoutX="162.0" layoutY="334.0" prefHeight="100.0" prefWidth="475.0" spacing="20.0">
         <children>
            <Button fx:id="saveButton" mnemonicParsing="false" onAction="#onSaveButtonClick" prefHeight="29.0" prefWidth="67.0" text="Save">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onCancelButtonClick" prefHeight="29.0" prefWidth="67.0" text="Cancel">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
